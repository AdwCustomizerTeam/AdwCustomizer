msgid "Blue"
msgstr "Azul"

#: src/settings_schema.py:320
msgid "Green"
msgstr "Verde"

#: src/settings_schema.py:321
msgid "Yellow"
msgstr "Amarillo"

#: src/settings_schema.py:322
msgid "Orange"
